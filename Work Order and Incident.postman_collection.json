									"pm.globals.set(\"WO_ID\", jsonData.id);",
									"pm.globals.set(\"WO_Code\", jsonData.code);",
									"",
									"",
									"var wo_Code = pm.globals.get(\"WO_Code\");",
									"pm.test(\"Body includes WorkOrder_Code value\", function () {",
									"    pm.expect(pm.response.text()).to.include(wo_Code);",
